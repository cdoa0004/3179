{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 300,
    "height": 300,
    "data": {
      "url": "https://raw.githubusercontent.com/cdoa0004/3179/refs/heads/main/states_aus.csv",
      "format": {"type": "csv"}
    },
    "mark": {
      "type": "arc",
      "tooltip": true  
    },
    "encoding": {
      "theta": {
        "field": "Evapotrans-piration (mm) 0000- 2400",
        "type": "quantitative",
        "title": "Evapotranspiration (mm)"
      },
      "color": {
            "field": "State",  
            "type": "nominal",
            "scale": {
              "domain": [
                  "New South Wales", 
                  "Northern Territory", 
                  "Queensland", 
                  "South Australia", 
                  "Tasmania", 
                  "Victoria", 
                  "Western Australia"
              ],
              "range": [
                  "#00be6fff", 
                  "#517cb7ff",  
                  "#fabe04ff",  
                  "#6bc8ccff",  
                  "#fe8c4eff",  
                  "#7e5f90ff",  
                  "#d35f91ff "  
              ]
          },
            "legend": {
              "title": "State", 
              "orient": "right"
            }
          },
      "tooltip": [  
        {"field": "State", "type": "nominal", "title": "State"},
        {"field": "Evapotrans-piration (mm) 0000- 2400", "type": "quantitative", "title": "Evapotranspiration (mm)", "format": ".2f"}
      ]
    }
  }
  