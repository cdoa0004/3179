{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 400,
    "height": 250,
    "data": {
      "url": "https://raw.githubusercontent.com/cdoa0004/3179/refs/heads/main/states_aus.csv",
      "format": {"type": "csv"}
    },
    "mark": "bar",
    "encoding": {
      "x": {"field": "State", "type": "nominal", "axis": {"title": "State"}},
      "y": {
        "field": "Average 10m Wind Speed (m/sec)",
        "type": "quantitative",
        "axis": {"title": "Average Wind Speed (m/sec)"}
      },
      "color": {
        "field": "State",  
        "type": "nominal",
        "scale": {
          "domain": [
              "New South Wales", 
              "Northern Territory", 
              "Queensland", 
              "South Australia", 
              "Tasmania", 
              "Victoria", 
              "Western Australia"
          ],
          "range": [
              "#00be6fff", 
              "#517cb7ff",  
              "#fabe04ff",  
              "#6bc8ccff",  
              "#fe8c4eff",  
              "#7e5f90ff",  
              "#d35f91ff "  
          ]
      },
        "legend": {
          "title": "State", 
          "orient": "right"
        }
      },
      "tooltip": [
        {"field": "State", "type": "nominal", "title": "State"},
        {"field": "Average 10m Wind Speed (m/sec)", "type": "quantitative", "title": "Average Wind Speed (m/sec)", "format": ".2f"}
      ]
    }
  }
  